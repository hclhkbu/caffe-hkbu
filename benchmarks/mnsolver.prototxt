# The train/test net protocol buffer definition
net: "fcn5mnLAYER-bBATCHSIZE.prototxt"
base_lr: LR 
lr_policy: "fixed"
max_iter: 16 
display: 1 
solver_mode: GPU 
momentum: 0.9

# covering the full 10,000 testing images.
test_iter: 100
# Carry out testing every 1 training epochs.
test_interval: 100 
